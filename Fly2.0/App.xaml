<Application x:Class="Fly2._0.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Fly2._0"
             xmlns:view="clr-namespace:Fly2._0.MVVM.View"
             xmlns:viewModel="clr-namespace:Fly2._0.MVVM.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/MenuButtonTheme.xaml" />
                <ResourceDictionary Source="Theme/ScrollViewerStyle.xaml" />
                <ResourceDictionary Source="Theme/ButtonStyle.xaml" />
                <ResourceDictionary Source="Theme/ComboBoxStyle.xaml" />
                <ResourceDictionary Source="Theme/RoundedButton.xaml" />
                <ResourceDictionary Source="Theme/RoundedTextBox.xaml" />
                <ResourceDictionary Source="Theme/GridStyle.xaml" />
                <ResourceDictionary Source="Theme/ToolBarStyle.xaml" />
                <ResourceDictionary Source="Theme/DatePickerStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <DataTemplate DataType="{x:Type viewModel:MainWindowViewModel}">
                <view:MainWindowView/>
            </DataTemplate>            
            <DataTemplate DataType="{x:Type viewModel:SearchViewModel}">
                <view:SearchView/>
            </DataTemplate>            
            <DataTemplate DataType="{x:Type viewModel:BookingStatusViewModel}">
                <view:BookingStatusView/>
            </DataTemplate>            
            <DataTemplate DataType="{x:Type viewModel:Chek_inViewModel}">
                <view:Chek_inView/>
            </DataTemplate>           
            <DataTemplate DataType="{x:Type viewModel:RegistrationViewModel}">
                <view:RegistrationView/>
            </DataTemplate> 
            <DataTemplate DataType="{x:Type viewModel:AddViewModel}">
                <view:AddView/>
            </DataTemplate>          
            <DataTemplate DataType="{x:Type viewModel:DeleteViewModel}">
                <view:DeleteView/>
            </DataTemplate>          
            <DataTemplate DataType="{x:Type viewModel:ChangeViewModel}">
                <view:ChangeView/>
            </DataTemplate>          
            <DataTemplate DataType="{x:Type viewModel:ReportViewModel}">
                <view:ReportView/>
            </DataTemplate>            
            <DataTemplate DataType="{x:Type viewModel:OrdersViewModel}">
                <view:OrdersView/>
            </DataTemplate>            
            <DataTemplate DataType="{x:Type viewModel:AircraftViewModel}">
                <view:AircraftView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type viewModel:AirticketsViewModel}">
                <view:AirticketsView/>
            </DataTemplate>           
            <DataTemplate DataType="{x:Type viewModel:FlightViewModel}">
                <view:FlightView/>
            </DataTemplate>            
            <DataTemplate DataType="{x:Type viewModel:TicketTypeViewModel}">
                <view:TicketTypeView/>
            </DataTemplate>           
            <DataTemplate DataType="{x:Type viewModel:UsersViewModel}">
                <view:UsersView/>
            </DataTemplate>            
            <DataTemplate DataType="{x:Type viewModel:AddressViewModel}">
                <view:AddressView/>
            </DataTemplate>            
            <DataTemplate DataType="{x:Type viewModel:ContactViewModel}">
                <view:ContactView/>
            </DataTemplate>           
            <DataTemplate DataType="{x:Type viewModel:PassportViewModel}">
                <view:PassportView/>
            </DataTemplate>            
            <DataTemplate DataType="{x:Type viewModel:ChooseFlightViewModel}">
                <view:ChooseFlightView/>
            </DataTemplate>          
            <DataTemplate DataType="{x:Type viewModel:ChooseTicketTypeViewModel}">
                <view:ChooseTicketTypeView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type viewModel:BookTicketViewModel}">
                <view:BookTicketView/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
